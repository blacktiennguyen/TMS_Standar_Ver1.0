<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@MessageManager.GetCaptionValueByKey("lblTitleTMS") | 404 @MessageManager.GetCaptionValueByKey("lblPageNotFound")</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            404 @MessageManager.GetCaptionValueByKey("lblErrorPage")
        </h1>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="error-page">
            <h2 class="headline text-yellow"> 404</h2>
            <div class="error-content">
                <h3><i class="fa fa-warning text-yellow"></i> @MessageManager.GetCaptionValueByKey("lblPageNotFound")</h3>
                <p>
                    @MessageManager.GetCaptionValueByKey("lblContentPageNotFound")
                    @MessageManager.GetCaptionValueByKey("lblMeanwhileyoumay") <a href="/">@MessageManager.GetCaptionValueByKey("lblReturnToDashboard")</a>
                </p>
            </div>
            <!-- /.error-content -->
        </div>
        <!-- /.error-page -->
    </section>
    <!-- /.content -->
</body>
</html>